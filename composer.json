{
  "name" : "optimiza/ilab-media-tools",
  "description": "Tools for cropping, uploading to S3, serving through Imgix",
  "type": "wordpress-plugin",
  "require": {
    "php": ">=5.4.16",
    "composer/installers" : "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "license": "LGPL",
  "keywords": ["s3","imgix","cropping", "wordpress"],
  "repositories": [{
    "type": "composer",
    "url": "https://packages.interfacelab.io"
  }],
  "authors": [
    {
      "name": "Jon Gilkison",
      "email": "jon@interfacelab.com"
    }
  ],
  "require" : {
    "imgix/imgix-php": "^1.0",
    "smalot/pdfparser": "^0.10.0",
    "fasterimage/fasterimage": "^1.1",
    "monolog/monolog": "^1.23",
    "google/cloud-storage": "^1.1",
    "cwhite92/b2-sdk-php": "^1.3",
    "ilab/ilab-aws-media-cloud-sdk": "^0.1.4"
  },
  "autoload": {
    "psr-4": {
      "ILAB\\MediaCloud\\": "classes"
    },
    "files": [
      "classes/Utilities/Helpers.php"
    ]
  }
}

